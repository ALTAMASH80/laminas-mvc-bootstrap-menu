{
    "name": "altamash80/laminas-mvc-bootstrap-menu",
    "description": "Validate and display simple and complex forms, casting forms to business objects and vice versa",
    "license": "GNU",
    "keywords": [
        "laminas",
        "bootstrap",
        "lrphpt"
    ],
    "homepage": "http://www.lrphpt.com",
    "authors":[
      {
        "name":"Shah Mubashir Hussain",
        "email":"shah.mubashir@gmail.com"
      }
    ],
    "support": {
        "issues": "https://github.com/ALTAMASH80/laminas-mvc-bootstrap-menu/issues",
        "source": "https://github.com/ALTAMASH80/laminas-mvc-bootstrap-menu",
        "rss": "https://github.com/ALTAMASH80/laminas-mvc-bootstrap-menu/releases.atom"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "lrphpt": {
            "component": "LRPHPT",
            "config-provider": "LRPHPT\\ConfigProvider"
        }
    },
    "require": {
        "php" : "^7.3 || ^8.0",
        "laminas/laminas-modulemanager" : "^2.5",
        "laminas/laminas-loader" : "^2.6",
        "laminas/laminas-eventmanager" : "^3.0",
        "laminas/laminas-mvc" : "^3.0",
        "laminas/laminas-navigation" : "^2.10"
    },
    "require-dev": {
        "laminas/laminas-escaper": "^2.5",
        "laminas/laminas-filter": "^2.6",
        "laminas/laminas-i18n": "^2.6",
        "laminas/laminas-servicemanager": "^2.7.5 || ^3.0.3",
        "laminas/laminas-text": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "LRPHPT\\": "src/"
        }
    }
}
